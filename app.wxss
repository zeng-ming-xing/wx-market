/**app.wxss**/
@import './styles/iconfont.wxss';
/*在微信小程序中不识别通配符 * */
page,view,text,swiper,seiper-item,image,navigator{
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}
/*主题颜色  通过变量来实现
原生的css和wxss都支持变量
*/
page{
  /*使用变量存储主题颜色*/
  --themeColor:#eb4450;
  /*同一定义字体大小  
    1px=2rpx
    14px=28rpx
  */
  font-size: 28rpx;
}
image{
  width: 100%;
}
