<table tabs="{{tables}}" bindtabsItemChange="handchange" index="{{index}}">
  <view class="order_main">
    <view wx:for="{{orders}}" wx:key="order_id" class="order_item">
      <view class="order_num">
        <veiw class="num_item">订单编号</veiw>
        <veiw class="num_value">{{item.order_number
        }}</veiw>
       </view>  
       <view class="order_price">
        <veiw class="price_item">订单价格</veiw>
        <veiw class="price_value">￥{{item.order_price}}</veiw>
      </view>
      <view class="order_date">
        <veiw class="date_item">订单日期</veiw>
        <veiw class="date_value">{{item.create_time_cn}}</veiw>
      </view>
    </view>
  </view>
</table>